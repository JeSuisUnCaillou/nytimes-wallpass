{
    "name": "nytimes-wallpass",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Bypass the New York Times frontend paywall",
    "content_scripts": [
        {
          "matches": [
            "https://www.nytimes.com/*",
            "http://www.nytimes.com/*"
          ],
          "js": ["js/content.js"]
        }
    ]
}